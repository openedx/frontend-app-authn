    "algoliasearch": "^4.14.3",
    "algoliasearch-helper": "^3.14.0",
    "classnames": "2.5.1",
    "core-js": "3.41.0",
    "fastest-levenshtein": "1.0.16",
    "form-urlencoded": "6.1.5",
    "prop-types": "15.8.1",
    "query-string": "7.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "6.1.0",
