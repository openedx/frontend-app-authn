    "classnames": "2.3.2",
    "core-js": "3.26.1",
    "extract-react-intl-messages": "4.1.1",
    "fastest-levenshtein": "1.0.16",
    "form-urlencoded": "4.2.1",
    "lodash.camelcase": "4.3.0",
    "lodash.snakecase": "4.1.1",
    "prop-types": "15.8.1",
    "query-string": "5.1.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
