    "reselect": "4.1.5",
    "sanitize-html": "2.7.0",
    "semver-regex": "3.1.3",
    "universal-cookie": "4.0.4"
  },
  "devDependencies": {
    "@edx/frontend-build": "9.1.2",
    "@edx/reactifex": "1.0.3",
    "babel-plugin-formatjs": "10.3.18",
    "codecov": "3.8.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "es-check": "6.2.1",
    "glob": "7.2.0",
